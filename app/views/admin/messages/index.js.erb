var message = $('#message_message_text').val();
var name = $('#message_name').val();
var newMessage = $.ajax({
  data: { 
        'message[message_text]': message,
      'message[name]': name
    },
  type: 'post',
  url: '<%= j admin_organization_messages_path(params[:organization_id]) %>'
});
